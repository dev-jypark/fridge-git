<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<!-- namespace속성:매퍼파일의 완전한경로 .xml는 생략 -->
<!-- ※ibatis와는 다르게 id값에 .(dot)를 사용 못한다. --> 
<mapper namespace="fridge.mybatis.mapper.image">
	<select id="imageSelectList" parameterType="Map" resultType="imageDTO">
		SELECT t.*
		FROM member m JOIN t_imgsrc t ON m.id=t.id
		WHERE tb_no=#{no}
		ORDER BY ti_no DESC
	</select>
</mapper>